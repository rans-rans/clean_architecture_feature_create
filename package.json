{
	"name": "create_fca_feature",
	"displayName": "New Clean architecture Feature",
	"description": "This is an extension for flutter that enables you to create create a template for a feature in using the clean architecture standards",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/rans-rans/clean_architecture_feature_create"
	},
	"engines": {
		"vscode": "^1.93.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:flutter-ca-extension.create_fca_feature"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "create_fca_feature.create_feature",
				"title": "Create Feature"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "create_fca_feature.create_feature",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.93.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}